{
  "name": "react-modular-map",
  "version": "0.0.3",
  "description": "Modular ReactJS Map (use with providers)",
  "main": "lib/index.js",
  "typings": "lib/index",
  "scripts": {
    "clean": "rm -rf lib lib_es6 umd __tmp__",
    "lint": "$(npm bin)/tslint",
    "prepublish": "npm run clean && npm run build",
    "_serve_examples": "$(npm bin)/webpack-dev-server --config examples/webpack.config.js",
    "_watch_ts_for_examples": "$(npm bin)/tsc --watch",
    "serve_examples": "npm run _serve_examples & npm run _watch_ts_for_examples",
    "build_commonjs": "npm run clean && node_modules/.bin/tsc",
    "watch:build_commonjs": "npm run clean && node_modules/.bin/tsc --project tsconfig_lib.json --watch",
    "build_umd": "npm run clean && node_modules/.bin/tsc --project tsconfig_lib.json --module umd --outdir umd",
    "build_ts_lib_es6": "$(npm bin)/typings install && $(npm bin)/tsc --project tsconfig_lib.json",
    "lib_es6_to_lib": "$(npm bin)/babel lib_es6 -d lib && $(npm bin)/copyfiles -u 1 'lib_es6/*.d.ts' lib",
    "build": "npm run build_ts_lib_es6 && npm run lib_es6_to_lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdoomari/react-map.git"
  },
  "keywords": [
    "react",
    "map"
  ],
  "author": "devdoomari@gmail.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/devdoomari/react-map/issues"
  },
  "homepage": "https://github.com/devdoomari/react-map#readme",
  "dependencies": {
    "eventemitter2": "^1.0.1",
    "keymirror": "^0.1.1",
    "lodash": "^4.9.0",
    "node-uuid": "^1.4.7",
    "q": "^1.4.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.3",
    "rxjs": "^5.0.0-beta.6",
    "typescript": "^1.8.10",
    "typings": "^0.7.12"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.2",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.3.17",
    "babel-jest": "^10.0.1",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "codemirror": "^5.9.0",
    "component-playground": "^1.2.0",
    "copyfiles": "^0.2.1",
    "core-decorators": "^0.12.2",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "flowcheck": "^0.2.7",
    "html-webpack-plugin": "^1.7.0",
    "jest-cli": "^0.10.0",
    "raw-loader": "^0.5.1",
    "react-bootstrap": "^0.28.5",
    "ts-loader": "^0.8.2",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "jest": {
    "testFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ]
  }
}
